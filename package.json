{
  "name": "@fect-ui/vue",
  "version": "0.1.0-alpha.5",
  "description": "Minimalist UI components built on Vue",
  "author": "Fect Org",
  "scripts": {
    "serve": "vue-cli-service serve",
    "test:unit": "vue-cli-service test:unit",
    "lint": "eslint \"{packages,_page,src,scripts}/**/*.{js,jsx,vue}\"",
    "collect": "node scripts/collect.js && eslint --fix --ext .js packages/index.js",
    "commit": "git-cz",
    "coverage": "npm run test:unit -- --coverage",
    "release": "npm publish --access public --non-interactive",
    "report": "npm run coverage && codecov",
    "setup": "hygen component new --name",
    "build:doc": "vue-cli-service build",
    "build:lib": "set NODE_OPTIONS=--max_old_space_size=8172 && set BABEL_ENV=production && rollup -c ./scripts/rollup.config.js",
    "build:types": "tsc -p ./scripts",
    "build": "npm run build:lib && npm run build:types"
  },
  "main": "lib/index.js",
  "module": "lib/index.js",
  "types": "types/index.d.ts",
  "files": [
    "LICENSE",
    "lib/",
    "types/"
  ],
  "dependencies": {
    "@fect-ui/themes": "^1.0.8-beta.0",
    "core-js": "^3.6.5",
    "vue": "^3.0.0"
  },
  "devDependencies": {
    "@babel/core": "^7.13.8",
    "@babel/plugin-transform-runtime": "^7.14.3",
    "@babel/preset-env": "^7.14.4",
    "@babel/preset-typescript": "^7.13.0",
    "@babel/runtime": "^7.14.0",
    "@fect-ui/vue-icons": "^0.1.2",
    "@mdx-js/vue-loader": "^1.6.22",
    "@rollup/plugin-babel": "^5.3.0",
    "@rollup/plugin-commonjs": "^19.0.0",
    "@rollup/plugin-node-resolve": "^13.0.0",
    "@types/jest": "^24.0.19",
    "@typescript-eslint/eslint-plugin": "^2.33.0",
    "@typescript-eslint/parser": "^2.33.0",
    "@vue/babel-plugin-jsx": "^1.0.3",
    "@vue/cli-plugin-babel": "~4.5.0",
    "@vue/cli-plugin-eslint": "~4.5.0",
    "@vue/cli-plugin-router": "~4.5.0",
    "@vue/cli-plugin-typescript": "^4.5.13",
    "@vue/cli-plugin-unit-jest": "~4.5.0",
    "@vue/cli-service": "~4.5.0",
    "@vue/compiler-sfc": "^3.0.0",
    "@vue/eslint-config-standard": "^5.1.2",
    "@vue/eslint-config-typescript": "^5.0.2",
    "@vue/test-utils": "^2.0.0-0",
    "babel-eslint": "^10.1.0",
    "babel-loader": "^8.2.2",
    "chalk": "^4.1.0",
    "codecov": "^3.8.2",
    "css-loader": "^5.1.1",
    "cz-conventional-changelog": "^3.3.0",
    "eslint": "^6.7.2",
    "eslint-config-kagura": "^1.0.3",
    "eslint-plugin-import": "^2.20.2",
    "eslint-plugin-node": "^11.1.0",
    "eslint-plugin-promise": "^4.2.1",
    "eslint-plugin-standard": "^4.0.0",
    "eslint-plugin-vue": "^7.0.0-0",
    "execa": "^5.0.0",
    "extract-mdx-metadata": "^1.0.0",
    "fs-extra": "^9.1.0",
    "less": "^3.0.4",
    "less-loader": "^5.0.0",
    "lint-staged": "^9.5.0",
    "postcss": "^8.3.0",
    "prismjs": "^1.23.0",
    "raw-loader": "^4.0.2",
    "rollup": "^2.50.5",
    "rollup-plugin-postcss": "^4.0.0",
    "rollup-plugin-terser": "^7.0.2",
    "rollup-plugin-typescript2": "^0.30.0",
    "rollup-plugin-vue": "^6.0.0",
    "style-loader": "^2.0.0",
    "tslib": "^2.2.0",
    "typescript": "~4.1.5",
    "vue-jest": "^5.0.0-0",
    "vue-loader": "^16.1.2",
    "vue-prism-component": "^2.0.0",
    "vue-router": "^4.0.8"
  },
  "peerDependencies": {
    "vue": "^3.0.0"
  },
  "config": {
    "commitizen": {
      "path": "./node_modules/cz-conventional-changelog"
    }
  },
  "gitHooks": {
    "pre-commit": "lint-staged"
  },
  "homepage": "https://github.com/fay-org/fect",
  "keywords": [
    "vue component",
    "fect",
    "vue ui"
  ],
  "license": "MIT",
  "lint-staged": {
    "*.{js,jsx,vue}": [
      "vue-cli-service lint",
      "git add"
    ]
  },
  "public": true,
  "repository": {
    "type": "git",
    "url": "https://github.com/fay-org/fect"
  }
}